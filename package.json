{
  "name": "ladda-preloader2",
  "version": "1.0.0",
  "description": "Angular2 wrapper for Ladda preloader",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rnadler/ng2-password-strength-bar.git"
  },
  "bugs": {
    "url": "https://github.com/rnadler/ng2-password-strength-bar/issues"
  },
  "homepage": "https://github.com/rnadler/ng2-password-strength-bar#readme",
  "scripts": {
    "start": "npm run build && concurrently \"tsc -w\" \"lite-server\" ",
    "e2e": "npm run build && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "pree2e": "webdriver-manager update",
    "test": "npm run build && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "npm run build && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "build": "rimraf lib && tsc -p src && tsc"
  },
  "keywords": [
    "angular",
    "angular2",
    "ladda",
    "preloader",
    "component"
  ],
  "author": "Dev Team Inc. <contact@dev-team.com> (http://dev-team.com)",
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "~2.2.0",
    "@angular/compiler": "~2.2.0",
    "@angular/core": "~2.2.0",

    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.10",

    "canonical-path": "0.0.2",
    "http-server": "^0.9.0",
    "tslint": "^4.0.2",
    "lodash": "^4.16.4",
    "jasmine-core": "~2.5.2",
    "karma": "^1.3.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "4.0.11",
    "webdriver-manager": "10.2.10",
    "rimraf": "^2.5.4",

    "@types/node": "^6.0.46",
    "@types/jasmine": "^2.5.36",
    "@types/selenium-webdriver": "^2.53.33",
    "@types/core-js": "^0.9.35",

    "angular-in-memory-web-api": "~0.1.15",
    "systemjs": "0.19.40",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.26"
  },
  "dependencies": {
    "ladda": "^1.0.0"
  }
}
